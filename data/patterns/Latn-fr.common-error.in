[Common Error Pattern]
_Name=Spaces around guillemets
_Description=Add spaces around guillemets
Classes=Human;OCR;
Pattern=(\S)(«)
Flags=DOTALL;MULTILINE;UNICODE;
Replacement=\1 \2
Repeat=False

[Common Error Pattern]
_Name=Spaces around guillemets
_Description=Add spaces around guillemets
Classes=Human;OCR;
Pattern=(»)(\w)
Flags=DOTALL;MULTILINE;UNICODE;
Replacement=\1 \2
